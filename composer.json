{
    "name": "pnwscm60/contao-pbwork-bundle",
    "description": "service recording",
    "keywords": [
        "contao",
        "bundle",
        "tag 3",
        "tag 4"
    ],
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Markus Schenker",
            "email": "ms@phi-network.ch",
            "homepage": "https://github.com/pnwscm60",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/pnwscm60/contao-pbwork-bundle/issues",
        "source": "https://github.com/pnwscm60/contao-pbwork-bundle"
    },
    "require": {
        "contao/core-bundle": "^4.9"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^4.0",
        "contao/manager-plugin": "^2.3.1",
        "contao/test-case": "^4.6",
        "phpunit/phpunit": "^9.3",
        "symfony/phpunit-bridge": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Pnwscm60\\ContaoPbworkBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Pnwscm60\\ContaoPbworkBundle\\ContaoManager\\Plugin"
    },
    "version": "1.0"
}